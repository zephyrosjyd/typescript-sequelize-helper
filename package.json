{
  "name": "orm-helper",
  "version": "0.1.0",
  "description": "Sequelize ORM Helpers with Typescript for boilerplates",
  "scripts": {
    "lint": "eslint -c .eslintrc --ext .ts ./app",
    "build": "tsc -b app --clean && tsc -b app",
    "dev": "ts-node --dir app src/server.ts",
    "debug": "node -r ts-node/register --inspect app/src/server.ts",
    "start": "node --trace-warnings built/server.js",
    "clean": "rm -rf built",
    "test": "jest --coverage --detectOpenHandles"
  },
  "engines": {
    "node": ">=14.0.0 ^14.5.0"
  },
  "license": "MIT",
  "author": "Luke Youngdong Ji <zephyrosjyd@gmail.com>",
  "jest": {
    "preset": "ts-jest",
    "rootDir": "./app",
    "setupFiles": ["<rootDir>/test/env"],
    "moduleFileExtensions": ["js", "json", "ts", "d.ts"]
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "lodash": "^4.17.15",
    "pg": "^8.2.1",
    "sequelize": "^6.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.3",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",
    "@types/pg": "^7.14.3",
    "@types/sequelize": "^4.28.9",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "plugin-import": "^1.0.52",
    "sequelize-cli": "^6.1.0",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
